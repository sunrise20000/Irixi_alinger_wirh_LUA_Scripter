<Window x:Class="Irixi_Aligner_Common.UserControls.WindowScript"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Irixi_Aligner_Common.UserControls"
        mc:Ignorable="d"
         Height="450" Width="800"
        DataContext="{Binding Source={StaticResource Locator}}"
        Title="{Binding StrFilePath,ElementName=ScriptEditer1}"
        Closing="Window_Closing">
    <Grid>
        <local:UC_ScriptEditer x:Name="ScriptEditer1"/>
    </Grid>
</Window>
